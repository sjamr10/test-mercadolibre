{
  "presets": ["stage-2", "env", "react"],
  "plugins": [
    "transform-class-properties",
    "jsx-control-statements",
    "transform-react-constant-elements",
    "transform-object-rest-spread",
    "syntax-object-rest-spread",
    "array-includes",
    [
      "module-resolver",
      {
        "root": ["./"]
      }
    ]
  ]
}
